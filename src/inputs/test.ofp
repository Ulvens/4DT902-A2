int sumUpTo(int n){
    int i = 1;
    int sum = 0;
    while (i < n+1) {
        sum = sum + i;
        i = i + 1;
    }
    return sum;
}


float max(float x, float y){
    if (x < y)
        return x;
    else
        return y;
}
